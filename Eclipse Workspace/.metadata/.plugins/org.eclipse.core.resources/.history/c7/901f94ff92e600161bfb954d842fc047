//life.c -- Niko Gamarra
#include <stdio.h>
#include <stdlib.h>
#include "Life.h"
FILE *input;

int main(int argc, char * argv[]) {
	if (argc < 5)
		printf("ERROR: Too few args.");
	//set up variables
	int x, y, gens;
	printf(argc[0]);
	x = atoi(argv[1]);
	y = atoi(argv[2]);
	gens = argv[3];
	int *B[];
	unsigned int i, j, k;
	B = malloc(x * sizeof(int *));
	if (B)
		for (i = 0; i < x; i++) {
			B[i] = malloc(y * sizeof(int));
			if (!B[i])
				exit(-1);
			/* error */
		}
	int c;

	input = (fopen(argv[4], "r");
	if (!input){
		printf("ERROR: File not found.");
	}else{
		while((c=getc(input))!= EOF){
				int x=0,y=0;
				if(c=="\n"){
					x=0;
					y++;
				}
				B[x][y]
				x++;

		}
		fclose(input)
	}

	int c;



	}

}
